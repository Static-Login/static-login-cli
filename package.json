{
  "name": "static-login-cli",
  "description": "Manage your staticly generated site.  Behind login.",
  "author": "Shaun Sweet",
  "homepage": "https://github.com/static-login/static-login-cli",
  "license": "MIT",
  "main": "",
  "repository": "static-login/static-login-cli",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "bin": {
    "static-login-cli": "./bin/run.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "1.1.4",
  "bugs": "https://github.com/static-login/static-login-cli/issues",
  "keywords": [
    "oclif"
  ],
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^4"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.1.0",
    "semantic-release": "^22.0.12",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
